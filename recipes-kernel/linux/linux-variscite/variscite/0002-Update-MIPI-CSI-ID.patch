From aeb3fba5bfcefabcb1fc5904ad8b4e96dc25565b Mon Sep 17 00:00:00 2001
From: Variscite <variscite@ub-rd-virt.(none)>
Date: Sun, 29 Jun 2014 12:26:49 +0300
Subject: [PATCH 02/13] Update MIPI CSI ID

---
 arch/arm/boot/dts/imx6qdl-var-som.dtsi | 6 +++---
 1 file changed, 3 insertions(+), 3 deletions(-)

diff --git a/arch/arm/boot/dts/imx6qdl-var-som.dtsi b/arch/arm/boot/dts/imx6qdl-var-som.dtsi
index 7c1927b..146918b 100644
--- a/arch/arm/boot/dts/imx6qdl-var-som.dtsi
+++ b/arch/arm/boot/dts/imx6qdl-var-som.dtsi
@@ -81,8 +81,8 @@
 		wlan_en_reg: fixedregulator@2 {
 			compatible = "regulator-fixed";
 			regulator-name = "wlan-en-regulator";
-			regulator-min-microvolt = <1800000>;
-			regulator-max-microvolt = <1800000>;
+			regulator-min-microvolt = <3300000>;
+			regulator-max-microvolt = <3300000>;
 
 			/* WLAN_EN GPIO for this board - Bank7, pin8 */
 			gpio = <&gpio7 8 0>;
@@ -563,7 +563,7 @@
 &mipi_csi {
 	status = "okay";
 	ipu_id = <0>;
-	csi_id = <0>;
+	csi_id = <1>;
 	v_channel = <0>;
 	lanes = <2>;
 };
